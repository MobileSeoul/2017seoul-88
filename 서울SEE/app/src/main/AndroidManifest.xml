<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.company.seoultest">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="MainActivity"
            android:label="MainActivity"></activity>
        <activity android:name="DiaryInsertActivity"
            android:label="DiaryInsertActivity"></activity>
        <activity android:name="DiaryActivity"
            android:label="DiaryActivity"></activity>
        <activity android:name=".PhotoCaptureActivity"
            android:label="PhotoCaptureActivity"></activity>
        <activity android:name="PhotoSelectionActivity"
            android:label="PhotoSelectionActivity"></activity>
        <activity android:name="SuitActivity"
            android:label="DiaryInsertActivity"></activity>
        <activity android:name="DetailActivity"
            android:label="DiaryInsertActivity"></activity>
    </application>

</manifest>